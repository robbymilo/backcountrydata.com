{
  "name": "backcountry-data-api",
  "version": "0.0.1",
  "description": "An API for accessing SNOTEL data in JSON format from the current water year. Data is cached and fetched every 15 minutes.",
  "keywords": [
    "snotel",
    "weather",
    "skiing",
    "snow"
  ],
  "author": {
    "name": "Robby Milo",
    "email": "robbymilo@fastmail.com"
  },
  "homepage": "https://backcountrydata.com",
  "private": false,
  "scripts": {
    "serve": "node app.js",
    "prettier": "prettier --check '**/*.js' --write"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "express": "^5.0.0",
    "geolib": "^3.3.3",
    "node-html-parser": "^5.4.2",
    "prettier": "^2.1.2",
    "xml-js": "^1.6.11"
  },
  "engines": {
    "node": "18.12.1"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": false,
    "arrowParens": "always"
  }
}
